{
  "name": "linkshortener",
  "version": "0.0.0",
  "repository": "https://github.com/bemijonathan/linkshortener",
  "license": "MIT",
  "main": "dist/server.js",
  "keywords": [],
  "scripts": {
    "start": "node dist/server.js",
    "start:dev": "cross-env NODE_ENV=production ts-node --transpile-only src/server.ts",
    "dev": "cross-env NODE_ENV=development nodemon --watch src --delay 1 --exec 'ts-node' src/server.ts",
    "dev2": "cross-env NODE_ENV=development nodemon src/server.ts",
    "test": "jest --forceExit --setupFiles dotenv/config --detectOpenHandles --coverage",
    "test:watch": "jest --setupFiles dotenv/config --watch --detectOpenHandles",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "build": "tsc -p ./"
  },
  "engines": {
    "node": "12.16.3"
  },
  "dependencies": {
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "envalid": "^7.0.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "hpp": "^0.2.3",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.5.0",
    "sequelize-typescript": "^2.1.0",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/hpp": "^0.2.1",
    "@types/jest": "^26.0.20",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.31",
    "@types/supertest": "^2.0.10",
    "@types/swagger-jsdoc": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.2",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "sucrase": "^3.17.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^4.1.5"
  }
}
